%canvas#dailySwipes{height: height}

:javascript
  window.labels2 = #{daily_swipes.keys.to_json.html_safe}
  window.values2 = #{daily_swipes.values.to_json.html_safe}
  window.data2 = {
    labels: labels2,
    datasets: [{
      label: 'Daily Swipes Per Day',
      backgroundColor: '#0d6efd',
      borderColor: 'rgb(255, 99, 132)',
      data: values2,
    }]
  };
  window.config2 = {
    type: 'bar',
    data: data2,
    options: {
      scales: {
        y: {
          beginAtZero: true
        },
        x: {
          title: {
            text: 'days back',
            display: true,
          }
        },
      }
    },
  };

  new Chart(document.getElementById('dailySwipes'), config2);
